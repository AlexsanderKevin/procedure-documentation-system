<!--departments-->
    {{#departments}}
        {{>edition/li_box title = this.dataValues.name class="department_list"}}
        <!--user-->
        {{#users}}
            {{>edition/li_box title = this.dataValues.name class="user_list"}}
        {{/users}}
        <!--sections-->
        {{#item_sections}}
            {{>edition/li_box title = this.dataValues.name class="section_list"}}
            <!--subsections-->
            {{#item_subsections}}
                {{>edition/li_box title = this.dataValues.name class="subsection_list"}}
                <!--procedures-->
                {{#items}}
                    {{>edition/li_box title = this.dataValues.title class="procedure_list"}}
                    <!--solutions-->
                    {{#item_solutions}}
                        {{>edition/li_box title = this.dataValues.title class="solution_list"}}
                    {{/item_solutions}}
                    <!--obs-->      
                    {{#item_obs}}
                        {{>edition/li_box title = this.dataValues.title class="obs_list"}}
                    {{/item_obs}}
                    <!--issues-->
                    {{#item_issues}}
                        {{>edition/li_box title = this.dataValues.title class="issue_list"}}
                    {{/item_issues}}
                {{/items}}
            {{/item_subsections}}
        {{/item_sections}}
    {{/departments}}