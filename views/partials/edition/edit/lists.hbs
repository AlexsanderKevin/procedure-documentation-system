<!--departments-->
    {{#departments}}
        {{>edition/li_box title = this.dataValues.name class="item_list department"}}
        <!--user-->
        {{#users}}
            {{>edition/li_box title = this.dataValues.name class="item_list user"}}
        {{/users}}
        <!--sections-->
        {{#item_sections}}
            {{>edition/li_box title = this.dataValues.name class="item_list section"}}
            <!--subsections-->
            {{#item_subsections}}
                {{>edition/li_box title = this.dataValues.name class="item_list subsection"}}
                <!--procedures-->
                {{#items}}
                    {{>edition/li_box title = this.dataValues.title class="item_list procedure"}}
                    <!--solutions-->
                    {{#item_solutions}}
                        {{>edition/li_box title = this.dataValues.title class="item_list solution"}}
                    {{/item_solutions}}
                    <!--obs-->      
                    {{#item_obs}}
                        {{>edition/li_box title = this.dataValues.title class="item_list obs"}}
                    {{/item_obs}}
                    <!--issues-->
                    {{#item_issues}}
                        {{>edition/li_box title = this.dataValues.title class="item_list issue"}}
                    {{/item_issues}}
                {{/items}}
            {{/item_subsections}}
        {{/item_sections}}
    {{/departments}}