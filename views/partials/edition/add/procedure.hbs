{{#>forms/default_form id="add_procedure_form" title="Novo Procedimento" confirm='Adicionar'}}
    {{>forms/input_box name="procedure_title" type="text" text="*Título"}}
    {{>forms/datalist_input name="procedure_subsection" text="*Subseção"}}
    <datalist name="procedure_subsection" id="procedure_subsection">
        {{#each item_subsections}}
            <option value="{{this.dataValues.name}}">
        {{/each}}
    </datalist>
    {{>forms/textarea_box name="procedure_description_input" text="*Descrição"}}
{{/forms/default_form}}